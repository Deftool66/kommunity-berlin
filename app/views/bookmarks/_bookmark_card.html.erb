<h1>here are all the Bookmarks</h1>
 <div>
    <% @bookmarks.each do |bookmark| %>
      <div class="card-product">
        <div class="card-product-infos">
          <h2><%= link_to bookmark.offer.title, offer_path(offer) %></h2>
          <h2><%= offer.description %></h2>
          <h2><%= offer.address %></h2>
        </div>
      </div>
    <% end %>
</div>
